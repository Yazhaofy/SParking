workspace 'MyWorkSpace.xcworkspace'
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!

target 'SParking' do
    pod 'AFNetworking'
    pod 'Masonry'
    pod 'SAMKeychain'
    
    pod 'ChameleonFramework'
    pod 'Wonderful'
    pod 'Colours'
    pod 'ColorExtension'
    
    pod 'EasyNavigation'
    pod 'EmptyDataSet'
    pod 'MBProgressHUD'
    pod 'QQ_XGPush'
    
    pod 'mob_sharesdk'
    pod 'mob_sharesdk/ShareSDKUI' # UI模块(非必须，提供的分享菜单栏和分享编辑页面)

    pod 'mob_sharesdk/ShareSDKPlatforms/QQ'
    pod 'mob_sharesdk/ShareSDKPlatforms/SinaWeibo'
    pod 'mob_sharesdk/ShareSDKPlatforms/WeChatFull' # 带支付

    pod 'mob_sharesdk/ShareSDKConfigurationFile' # 使用配置文件分享模块（非必需）
    pod 'mob_sharesdk/ShareSDKExtension' # 扩展模块（在调用可以弹出我们UI分享方法的时候是必需的）

    pod 'SMSSDK'
    
    ###########################################################################
    pod 'BaiduMapKit' #百度地图
    pod 'SDCycleScrollView' #轮播
    pod 'SDCountDownButton' #验证码倒计时按钮
    pod 'ABFRealmSearchViewController'
end
